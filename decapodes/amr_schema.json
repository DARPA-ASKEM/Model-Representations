{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$defs": {
    "MathML": {
      "oneOf": [
        {
          "$ref": "#/$defs/Math"
        },
        {
          "$ref": "#/$defs/Presentation"
        }
      ]
    },
    "Math": {
      "type": "object",
      "properties": {
        "tag": {
          "const": "Math"
        },
        "str": {
          "type": "string",
          "$comment": "Str"
        }
      },
      "required": [
        "str"
      ]
    },
    "Presentation": {
      "type": "object",
      "properties": {
        "tag": {
          "const": "Presentation"
        },
        "str": {
          "type": "string",
          "$comment": "Str"
        }
      },
      "required": [
        "str"
      ]
    },
    "ExpressionFormula": {
      "type": "object",
      "properties": {
        "expression": {
          "type": "string",
          "$comment": "Str"
        },
        "expression_mathml": {
          "$ref": "#/$defs/MathML"
        }
      },
      "required": [
        "expression",
        "expression_mathml"
      ]
    },
    "Unit": {
      "type": "object",
      "properties": {
        "expression": {
          "type": "string",
          "$comment": "Str"
        },
        "expression_mathml": {
          "$ref": "#/$defs/MathML"
        }
      },
      "required": [
        "expression",
        "expression_mathml"
      ]
    },
