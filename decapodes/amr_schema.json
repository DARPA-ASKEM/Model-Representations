    "Header": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string",
          "$comment": "Str"
        },
        "schema": {
          "type": "string",
          "$comment": "Str"
        },
        "description": {
          "type": "string",
          "$comment": "Str"
        },
        "schema_name": {
          "type": "string",
          "$comment": "Str"
        },
        "model_version": {
          "type": "string",
          "$comment": "Str"
        }
      },
      "required": [
        "name",
        "schema",
        "description",
        "schema_name",
        "model_version"
      ]
    }
  }
}