          "$ref": "#/$defs/StandardUniform"
        },
        {
          "$ref": "#/$defs/Uniform"
        },
        {
          "$ref": "#/$defs/StandardNormal"
        },
        {
          "$ref": "#/$defs/Normal"
        },
        {
          "$ref": "#/$defs/PointMass"
        },
        {
          "$ref": "#/$defs/Undefined"
        }
      ]
    },
    "StandardUniform": {
      "type": "object",
      "properties": {
        "tag": {
          "const": "StandardUniform"
        }
      },
      "required": [
      ]
    },
    "Uniform": {
      "type": "object",
      "properties": {
        "tag": {
          "const": "Uniform"
        },
        "min": {
          "type": "number",
          "$comment": "F64"
        },
        "max": {
          "type": "number",
          "$comment": "F64"
        }
      },
      "required": [
        "min",
        "max"
      ]
    },
    "StandardNormal": {
      "type": "object",
      "properties": {
        "tag": {
          "const": "StandardNormal"
        }
      },
      "required": [
      ]
    },
    "Normal": {
      "type": "object",
      "properties": {
        "tag": {
          "const": "Normal"
        },
        "mean": {
          "type": "number",
          "$comment": "F64"
        },
        "variance": {
          "type": "number",
          "$comment": "F64"
        }
      },
      "required": [
        "mean",
        "variance"
      ]
    },
    "PointMass": {
      "type": "object",
      "properties": {
        "tag": {
          "const": "PointMass"
        },
        "value": {
          "type": "number",
          "$comment": "F64"
        }
      },
      "required": [
        "value"
      ]
    },
    "Undefined": {
      "type": "object",
      "properties": {
        "tag": {
          "const": "Undefined"
        }
      },
      "required": [
      ]
    },
    "Observable": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "$comment": "Sym"
        },
        "name": {
          "type": "string",
          "$comment": "Str"
        },
        "states": {
          "type": "array",
          "items": {
            "type": "string",
            "$comment": "Sym"
          }
        },
        "f": {
          "$ref": "#/$defs/ExpressionFormula"
        }
      },
      "required": [
        "id",
        "name",
        "states",
        "f"
      ]
    },
    "Expression": {
      "oneOf": [
        {
          "$ref": "#/$defs/Rate"
        },
        {
          "$ref": "#/$defs/Initial"
        },
        {
          "$ref": "#/$defs/Parameter"
        },
        {
          "$ref": "#/$defs/Time"
        }
      ]
    },
    "Rate": {
      "type": "object",
      "properties": {
        "tag": {
          "const": "Rate"
