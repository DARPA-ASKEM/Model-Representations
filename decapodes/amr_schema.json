      },
      "required": [
        "target",
        "f"
      ]
    },
    "Initial": {
      "type": "object",
      "properties": {
        "tag": {
          "const": "Initial"
        },
        "target": {
          "type": "string",
          "$comment": "Sym"
        },
        "f": {
          "$ref": "#/$defs/ExpressionFormula"
        }
      },
      "required": [
        "target",
        "f"
      ]
    },
    "Parameter": {
      "type": "object",
      "properties": {
        "tag": {
          "const": "Parameter"
        },
        "id": {
          "type": "string",
          "$comment": "Sym"
        },
        "name": {
          "type": "string",
          "$comment": "Str"
        },
        "description": {
          "type": "string",
          "$comment": "Str"
        },
        "units": {
          "$ref": "#/$defs/Unit"
        },
        "value": {
          "type": "number",
          "$comment": "F64"
        },
        "distribution": {
          "$ref": "#/$defs/Distribution"
        }
      },
      "required": [
        "id",
        "name",
        "description",
        "units",
        "value",
        "distribution"
      ]
    },
    "Time": {
      "type": "object",
      "properties": {
        "tag": {
          "const": "Time"
        },
        "id": {
          "type": "string",
          "$comment": "Sym"
        },
        "units": {
          "$ref": "#/$defs/Unit"
        }
      },
      "required": [
        "id",
        "units"
      ]
    },
    "Note": {
      "oneOf": [
        {
          "$ref": "#/$defs/Name"
        },
        {
          "$ref": "#/$defs/Description"
        },
        {
          "$ref": "#/$defs/Grounding"
        },
        {
          "$ref": "#/$defs/Units"
        }
      ]
