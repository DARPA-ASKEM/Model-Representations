        "str": {
          "type": "string",
          "$comment": "Str"
        }
      },
      "required": [
        "str"
      ]
    },
    "Description": {
      "type": "object",
      "properties": {
        "tag": {
          "const": "Description"
        },
        "str": {
          "type": "string",
          "$comment": "Str"
        }
      },
      "required": [
        "str"
      ]
    },
    "Grounding": {
      "type": "object",
      "properties": {
        "tag": {
          "const": "Grounding"
        },
        "ontology": {
          "type": "string",
          "$comment": "Str"
        },
        "identifier": {
          "type": "string",
          "$comment": "Str"
        }
      },
      "required": [
        "ontology",
        "identifier"
      ]
    },
    "Units": {
      "type": "object",
      "properties": {
        "tag": {
          "const": "Units"
        },
        "expression": {
          "type": "string",
          "$comment": "Str"
        }
      },
      "required": [
        "expression"
      ]
    },
    "Annotation": {
      "type": "object",
      "properties": {
        "entity": {
          "type": "string",
          "$comment": "Sym"
        },
        "type": {
          "type": "string",
          "$comment": "Sym"
        },
        "note": {
          "$ref": "#/$defs/Note"
        }
      },
      "required": [
        "entity",
        "type",
        "note"
      ]
