      "required": [
        "f",
        "arg"
      ]
    },
    "App2": {
      "type": "object",
      "properties": {
        "tag": {
          "const": "App2"
        },
        "f": {
          "type": "string",
          "$comment": "Sym"
        },
        "arg1": {
          "$ref": "#/$defs/Term"
        },
        "arg2": {
          "$ref": "#/$defs/Term"
        }
      },
      "required": [
        "f",
        "arg1",
        "arg2"
      ]
    },
    "Plus": {
      "type": "object",
      "properties": {
        "tag": {
          "const": "Plus"
        },
        "args": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/Term"
          }
        }
      },
      "required": [
        "args"
      ]
    },
    "Mult": {
      "type": "object",
      "properties": {
