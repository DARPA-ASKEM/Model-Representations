        "rhs": {
          "$ref": "#/$defs/Term"
        }
      },
      "required": [
        "lhs",
        "rhs"
      ]
    },
    "DecaExpr": {
      "type": "object",
      "properties": {
        "context": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/Judgement"
          }
        },
        "equations": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/Equation"
          }
        }
      },
      "required": [
