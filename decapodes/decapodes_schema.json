    "Tan": {
      "type": "object",
      "properties": {
        "tag": {
          "const": "Tan"
        },
        "var": {
          "$ref": "#/$defs/Term"
        }
      },
      "required": [
        "var"
      ]
    },
    "Judgement": {
      "type": "object",
      "properties": {
        "var": {
          "type": "string",
          "$comment": "Sym"
