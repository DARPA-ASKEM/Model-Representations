{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$defs": {
    "Var": {
      "type": "object",
      "properties": {
        "tag": {
          "const": "Var"
        },
        "name": {
          "type": "string",
          "$comment": "Sym"
        }
      },
      "required": [
        "name"
      ]
    },
    "Lit": {
      "type": "object",
      "properties": {
        "tag": {
          "const": "Lit"
        },
        "name": {
          "type": "string",
          "$comment": "Sym"
        }
      },
      "required": [
        "name"
      ]
    },
    "AppCirc1": {
      "type": "object",
      "properties": {
        "tag": {
          "const": "AppCirc1"
        },
        "fs": {
          "type": "array",
          "items": {
            "type": "string",
            "$comment": "Sym"
          }
        },
        "arg": {
          "$ref": "#/$defs/Term"
        }
      },
      "required": [
        "fs",
        "arg"
      ]
    },
    "App1": {
      "type": "object",
      "properties": {
        "tag": {
